{
  "name": "calderawp/data",
  "description": "A boilerplate for new composer based PHP projects.",
  "license": "GPL-2.0",
  "require": {

  },
  "autoload": {
    "psr-4": {
      "calderawp\\caldera\\Data\\": "php"
    }
  },
  "require-dev": {

  },
  "config": {
    "platform": {
      "php": "7.2"
    },
    "sort-packages": true
  },
  "autoload-dev": {
    "psr-4": {
      "calderawp\\caldera\\Data\\Tests\\": "tests"
    }
  },
  "scripts": {
    "test": "phpunit tests",
    "tests-now": "phpunit tests --group=now",
    "sniffs": "phpcs src/ && phpcs src/",
    "fixes": "phpcbf src/ && phpcbf tests/"
  }
}
